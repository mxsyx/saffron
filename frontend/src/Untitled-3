    /**
     * 切换线路
     * @param {Number} channeId 线路ID
     */
    changeAddr: function(addrId) {
      this.addrs[this.activeAddr].hidden = true;
      this.addrs[addrId].hidden = false;
      this.activeAddr = addrId;
    },


    getPlayAddr(vid, addrId, index) {
      return `/play/${vid}/${addrId}/${index}`
    },